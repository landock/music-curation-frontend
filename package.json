{
  "name": "music-curation-frontend",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "cross-fetch": "^2.1.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "moment": "^2.21.0",
    "moment-duration-format": "^2.2.2",
    "node-sass-chokidar": "^1.2.2",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.4.1",
    "react-player": "^1.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "^0.8.3",
    "react-test-renderer": "^16.4.1",
    "react-transition-group": "^2.3.1",
    "recycle": "^3.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rxjs": "5.x",
    "seamless-immutable": "^7.1.3",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.0",
    "uid": "0.0.2"
  },
  "scripts": {
    "server": "PORT=3001 node -r babel-register server/index.js",
    "start-js": "export BABEL_ENV='DEVELOPMENT' && react-scripts start",
    "start": "npm-run-all -p watch-css start-js server",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "report": "yarn test -u --coverage ",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "postmerge": "if [[ $(git diff HEAD@{1}..HEAD@{0} -- package.json | wc -l) -gt 0 ]]; then yarn install; fi;"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "axios-mock-adapter": "^1.15.0",
    "babel-register": "^6.26.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "fetch-mock": "^6.3.0",
    "hygen": "^1.5.7",
    "jest-dom": "^1.3.1",
    "jest-fetch-mock": "^1.6.1",
    "prettier": "1.11.1",
    "prettier-eslint": "^8.8.1",
    "react-testing-library": "^3.1.7",
    "redux-mock-store": "^1.5.3",
    "storybook-addon-specifications": "^2.1.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "flow",
    "prettier.eslintIntegration": true
  },
  "quokka": {
    "babel": {
      "presets": ["env", "react"]
    } 
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.story.js",
      "!src/stories/index.js"
    ]
  }
}
